{
  "private": true,
  "name": "functionaljs",
  "version": "1.0.0",
  "description": "Experimental work from the book Professor Frisby's Mostly Adequate Guide to Functional Programming by Professor Franklin Risby. Requires ECMAscript 6 version of nodejs or a transpiler.",
  "main": "app.js",
  "engines": {
    "node": "4.2.1"
  },
  "scripts": {
    "postinstall": "ln -s ../../config/pre-commit .git/hooks/pre-commit; grunt",
    "test": "grunt coverage",
    "start": "export NODE_ENV=production; forever start ./config/forever.prod.json",
    "stop": "forever stop functionaljs-prod",
    "restart": "forever restart functionaljs-prod",
    "pretest": "grunt check",
    "serve": "export NODE_ENV=development;         nodemon --watch app.js --watch app/ --delay 1 --verbose",
    "debug": "export NODE_ENV=development; DEBUG=* nodemon --watch app.js --watch app/ --delay 1 --verbose",
    "debugger": "export NODE_ENV=development; DEBUG=* node-debug app.js",
    "debugtest": "export NODE_DEBUG=module; DEBUG=* grunt coverage",
    "debuggertest": "DEBUG=* node-debug _mocha test/setup.js test/app/Functional.spec.js",
    "covereach": "for plan in `find test/app/ -name *.spec.js`; do echo === $plan ===================================; rm -rf doc/coverage/*; grunt coverage --plan $plan; done",
    "coverage-view": "chromium-browser doc/coverage/index.html",
    "edit": "subl Gruntfile.js app.js package.json README.md `find app/ test/ -type f`",
    "performance": "siege --internet --file test/urls.lst --concurrent 100 --time 1M",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flowold": "flow --old-output-format; test $? -eq 0 -o $? -eq 2"
  },
  "author": "Brent Cowgill",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcowgill/functionaljs"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "debug": "^2.2.0",
    "forever": "^0.15.1",
    "node-noop": "^1.0.0"
  },
  "devDependencies": {
    "accounting": "^0.4.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.1",
    "data.maybe": "^1.2.1",
    "data.task": "^3.0.0",
    "debug": "^2.2.0",
    "fantasy-land": "^0.2.1",
    "flow-bin": "^0.22.1",
    "forever": "^0.15.1",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-flow": "^1.0.3",
    "grunt-mocha-chai-sinon": "0.0.9",
    "grunt-mocha-istanbul": "^3.0.1",
    "grunt-run": "^0.5.2",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.1.0",
    "leftpad": "0.0.0",
    "lodash.curry": "^4.0.0",
    "memoize": "^0.1.1",
    "mocha": "^2.4.5",
    "node-noop": "^1.0.0",
    "nodemon": "^1.9.1",
    "ramda": "^0.19.1",
    "ramda-alias": "^0.1.1",
    "ramda-cli": "^1.1.1",
    "ramda-debug": "^0.1.3",
    "ramda-destruct": "^1.1.3",
    "ramda-ext": "0.0.3",
    "ramda-extended": "^0.3.5",
    "ramda-extras": "^0.2.3",
    "ramda-fantasy": "^0.4.1",
    "ramda-fantasy-validation": "^0.1.0",
    "ramda-mapobjdeep": "^1.0.1",
    "ramda-maybe": "^0.18.0",
    "ramda-repl": "^3.0.8",
    "ramda-t": "0.0.18",
    "rcb": "0.0.11",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "sweet-ramda": "0.0.2",
    "underscore": "^1.8.3"
  }
}
